    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.1.10",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.4",
    "multer": "^1.4.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "pg-promise": "^10.11.1",
    "supertest": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/register": "^7.16.9",
    "jshint": "^2.13.3"
  },
  "scripts": {
    "start": "nodemon src/index.js",
    "pretest": "npm run migrate:down -e test && npm run migrate:up -e test",
    "test": "cross-env NODE_ENV=test nyc --require @babel/register --reporter=lcov --reporter=text --reporter=text-summary mocha --exit --recursive --timeout=150000 test/**/*.js",
    "posttest": "npm run migrate:down",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down -c 100"

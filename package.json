  "nyc": {
    "all": false,
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/migrations/**",
      "src/config/**"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
